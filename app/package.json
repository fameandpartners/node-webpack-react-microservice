{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=3030 node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --notify",
    "test:once": "CI=true yarn test -- --coverage",
    "server_test": "NODE_ENV='production' mocha --compilers js:babel-register --require ./src/lib/testNullCompiler.js && jest --coverage",
    "production": "node scripts/build.js && node server.js",
    "local_production": "node scripts/build.js && nodemon server.js",
    "precommit": "yarn test:once",
    "prepush": "yarn test:once"
  },
  "keywords": [
    "react",
    "markup",
    "ssr",
    "server side rendering",
    "webpack"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|server_test)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.scss$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|scss|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "babel-cli": "^6.24.0",
    "babel-core": "6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-app": "^2.2.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "custom-event": "^1.0.1",
    "express": "^4.15.2",
    "helmet": "^3.6.1",
    "ignore-styles": "^5.0.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "mirror-creator": "^1.1.0",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "raf": "^3.3.2",
    "rapscallion": "^2.1.7",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.5.2",
    "react-autobind": "^1.0.6",
    "react-dom": "^15.4.2",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.4",
    "redis": "^2.7.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.3",
    "shallowequal": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "2",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "husky": "^0.13.3",
    "isomorphic-webpack": "^2.0.2",
    "jest": "^19.0.2",
    "json-loader": "0.5.4",
    "loader-utils": "^1.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "pascal-case": "^2.0.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "style-loader": "0.13.1",
    "stylelint-scss": "^1.4.4",
    "stylelint-webpack-plugin": "^0.7.1",
    "svg-sprite-loader": "^3.0.5",
    "url-loader": "0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "engines": {
    "node": "7.6.0"
  }
}
